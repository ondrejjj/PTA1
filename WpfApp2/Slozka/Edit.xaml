<Window x:Class="WpfApp2.Slozka.Edit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp2.Slozka"
        mc:Ignorable="d"
        Title="Edit" Height="450" Width="800">
    <Grid>
        <TextBox
 x:Name="ProNazev"
 HorizontalAlignment="Center" 
 Margin="0,49,0,0"
 TextWrapping="Wrap"
 Text="{Binding Jmeno, Mode=OneWay}" 
 VerticalAlignment="Top"
 Width="120"/>

        <TextBox 
 x:Name="ProPocet"
 HorizontalAlignment="Center" 
 Margin="0,101,0,0"
 TextWrapping="Wrap"
 Text="{Binding Pocet, Mode=OneWay}"
 VerticalAlignment="Top" 
 Width="120"/>

        <TextBox 
 x:Name="ProCena"
 HorizontalAlignment="Center" 
 Margin="0,101,0,0"
 TextWrapping="Wrap"
 Text="{Binding Cena, Mode=OneWay}"
 VerticalAlignment="Top" 
 Width="120"/>



        <Button Content="Uložit"
     HorizontalAlignment="Left"
     Margin="99,221,0,0"
     VerticalAlignment="Top"
     Click="Button_Click"
     Width="60"/>

        <Button Content="Zahodit"
     HorizontalAlignment="Left"
     Margin="238,221,0,0"
     VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
</Window>
